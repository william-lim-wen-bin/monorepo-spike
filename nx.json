{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{workspaceRoot}/package.json",
      "{workspaceRoot}/pnpm-lock.yaml"
    ]
  },
  "targetDefaults": {
    "test": {
      "cache": true
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node {projectRoot}/src/index.js"
      }
    }
  }
}
